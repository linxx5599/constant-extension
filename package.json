{
  "name": "constant-extension",
  "displayName": "constant-extension",
  "description": "Converts the selected content to uppercase",
  "publisher": "constant-extension-linjy",
  "icon": "constant.png",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "upperCamelCase",
        "title": "=> UpperCamelCase"
      },
      {
        "command": "lowerCamelCase",
        "title": "=> lowerCamelCase"
      },
      {
        "command": "lowerCase",
        "title": "=> lowercase"
      },
      {
        "command": "upperCase",
        "title": "=> UPPERCASE"
      },
      {
        "command": "lowerSnakeCase",
        "title": "=> lower_snake_case"
      },
      {
        "command": "upperSnakeCase",
        "title": "=> UPPER_SNAKE_CASE"
      },
      {
        "command": "lowerKebabCase",
        "title": "=> lower-kebab-case"
      },
      {
        "command": "upperKebabCase",
        "title": "=> UPPER-KEBAB-CASE"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "group": "navigation",
          "submenu": "root"
        }
      ],
      "root": [
        {
          "command": "upperCamelCase",
          "group": "rename"
        },
        {
          "command": "lowerCamelCase",
          "group": "rename"
        },
        {
          "command": "lowerCase",
          "group": "rename"
        },
        {
          "command": "upperCase",
          "group": "rename"
        },
        {
          "command": "lowerSnakeCase",
          "group": "rename"
        },
        {
          "command": "upperSnakeCase",
          "group": "rename"
        },
        {
          "command": "lowerKebabCase",
          "group": "rename"
        },
        {
          "command": "upperKebabCase",
          "group": "rename"
        }
      ]
    },
    "submenus": [
      {
        "id": "root",
        "label": "Rename"
      }
    ],
    "keybindings": [
      {
        "command": "upperSnakeCase",
        "key": "ctrl+alt+q",
        "mac": "cmd+alt+q"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "wordsninja": "^1.0.0"
  },
  "repository": {
        "type": "git",
        "url": "https://github.com/linxx5599/constant-extension.git"
    }
}
